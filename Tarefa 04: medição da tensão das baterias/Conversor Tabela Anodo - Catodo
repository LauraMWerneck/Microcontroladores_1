#include <stdio.h>
#include <stdlib.h>
#include<stdint.h>


const uint8_t convTable[] = {0x40, 0x79, 0x24, 0x30, 0x19, 0x12, 0x02,
        0x78, 0x00, 0x18, 0x08, 0x03, 0x46, 0x21, 0x06, 0x0E};

int main()
{
    for(int i=0; i< sizeof(convTable); i++ ){

    printf("0x%x, ", ~convTable[i] & 0xff);

    }


    return 0;
}
